    uart3_pins_default: uart3_pins_default {
        pinctrl-single,pins = <
            0x3f8 (PIN_INPUT_SLEW | MUX_MODE2) /* uart2_ctsn.uart3_rxd */
            0x3fc (PIN_INPUT_SLEW | MUX_MODE1) /* uart2_rtsn.uart3_txd */
        >;
    };
    mmc1_pins_default: mmc1_pins_default {
